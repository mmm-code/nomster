<div class="booyah-box col-10 offset-1">
  <h1>Edit Your Place</h1>
  <%= simple_form_for @place do |f| %>
    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :description %>
    <%= f.file_field :image %>
    <%= f.label :remote_image_url, "or image URL" %>
    <%= f.text_field :remote_image_url %>
    <br />
    <%= f.submit 'Update', class: 'btn btn-primary' %>
   <br/>
  <br/>
  <br/>
  <br/>
  <br/>
  <% end %>
  
</div>